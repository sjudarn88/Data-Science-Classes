        "# when the null hypothesis is true (false alarm) 5% of the time\n",
        "num_tests = 10000\n",
        "results = np.zeros(num_tests)\n",
        "for i in range(num_tests):\n",
        "    x1 = np.random.randn(100)\n",
        "    x2 = np.random.randn(100)\n",
        "    z,p = ztest(x1,x2)\n",
        "    results[i] = (p<0.05)\n",
