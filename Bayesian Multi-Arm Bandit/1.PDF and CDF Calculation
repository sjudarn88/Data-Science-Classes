      "source": [
        "# two-sample test\n",
        "N0 = 100\n",
        "mu0 = 0.2\n",
        "sigma0 = 1\n",
        "x0 = np.random.randn(N0)*sigma0 + mu0\n",
        "\n",
        "N1 = 100\n",
        "mu1 = 0.5\n",
        "sigma1 = 1\n",
        "x1 = np.random.randn(N1)*sigma1 + mu1"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
