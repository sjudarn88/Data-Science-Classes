      "source": [
        "# two-sided test\n",
        "mu_hat = x.mean()\n",
        "sigma_hat = x.std(ddof=1)\n",
        "z = (mu_hat-0)/(sigma_hat/np.sqrt(N))\n",
        "p_right = 1-norm.cdf(np.abs(z))\n",
        "p_left = norm.cdf(-np.abs(z))\n",
        "p = p_right + p_left\n",
        "z,p"
      ],
      "execution_count": null,
